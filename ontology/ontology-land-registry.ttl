cad:taxpayerFirstName
    a owl:DatatypeProperty ;
    rdfs:domain cad:Taxpayer ;
    rdfs:range xsd:string ;
    rdfs:comment "Prénom(s) d'un contribuable"@fr,
                 "First name(s) of a taxpayer"@en ;
    rdfs:label "prénom(s)"@fr, "first name(s)"@en .

cad:taxpayerStatus a owl:DatatypeProperty ;
    rdfs:domain cad:Taxpayer ;
    rdfs:range xsd:string ;
    rdfs:comment "Statut d'un contribuable (M, Mme, Veuve, Veuf, Fils, Fille, Mineur, Héritiers)"@fr ,
                 "Status of a taxpayer (Mr, Mrs, Widow, Widower, Son, Daughter, Minor, Heirs)"@en ;
